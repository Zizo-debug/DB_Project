                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
    AVG(CAST(REPLACE(REPLACE(LEFT(SalaryRange, CHARINDEX('-', SalaryRange) - 1), 'k', ''), 'K', '') AS FLOAT) * 1000) AS AverageMinSalary,
    AVG(CAST(REPLACE(REPLACE(SUBSTRING(
        SalaryRange,
        CHARINDEX('-', SalaryRange) + 1,
        CHARINDEX(' ', SalaryRange + ' ') - CHARINDEX('-', SalaryRange) - 1
    ), 'k', ''), 'K', '') AS FLOAT) * 1000) AS AverageMaxSalary
FROM 
    Interviews i
JOIN 
    Applications a ON i.ApplicationID = a.ApplicationID
JOIN 
    JobPostings j ON a.JobID = j.JobID
WHERE 
    i.Result = 'Selected'
    AND j.SalaryRange LIKE '%-%';
</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
